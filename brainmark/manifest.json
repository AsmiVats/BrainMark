{
    "manifest_version": 3,
    "name": "Brainmark",
    "version": "1.0",
    "description": "Save and organize bookmarks by category.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["tabs", "storage"],
    "icons": {
      "48": "icon.png"
    }
  }
  